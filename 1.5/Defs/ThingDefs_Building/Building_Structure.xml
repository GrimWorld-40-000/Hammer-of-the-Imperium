<Defs>

	<ThingDef ParentName="BuildingBase">
		<defName>GW_HOI_Embrasure</defName>
		<label>imperium embrasure</label>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<description>A wall with a firing port to allow shooting from relative safety.</description>
		<graphicData>
			<texPath>Things/Building/Linked/Embrasure/GW_OrdinaryWalls_Emb_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkType>CornerFiller</linkType>
			<linkFlags>
				<li>Wall</li>
				<!-- <li>Rock</li> -->
			</linkFlags>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
				<edgeTop>Damage/Edge</edgeTop>
				<edgeBot>Damage/Edge</edgeBot>
				<edgeLeft>Damage/Edge</edgeLeft>
				<edgeRight>Damage/Edge</edgeRight>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Linked/Embrasure/GW_OrdinaryWalls_Emb_Icon</uiIconPath>
		<!-- <uiIconPathsStuff> -->
		<!-- <li> -->
		<!-- <appearance>Planks</appearance> -->
		<!-- <iconPath>Things/Building/Linked/VanillaEmbrasure/HRK_EmbrasurePlanks_Icon</iconPath> -->
		<!-- </li> -->
		<!-- <li> -->
		<!-- <appearance>Bricks</appearance> -->
		<!-- <iconPath>Things/Building/Linked/VanillaEmbrasure/HRK_EmbrasureBricks_Icon</iconPath> -->
		<!-- </li> -->
		<!-- </uiIconPathsStuff> -->
		<statBases>
			<MaxHitPoints>250</MaxHitPoints>
			<WorkToBuild>190</WorkToBuild>
			<Flammability>0.0</Flammability>
		</statBases>
		<researchPrerequisites>
			<li>GW_AM_ImperiumDefense</li>
		</researchPrerequisites>
		<costStuffCount>5</costStuffCount>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>0.70</fillPercent>
		<coversFloor>true</coversFloor>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<tickerType>Never</tickerType>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<useStuffTerrainAffordance>true</useStuffTerrainAffordance>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
		<holdsRoof>true</holdsRoof>
        <designationCategory>GrimworldCategory</designationCategory>
		<staticSunShadowHeight>1</staticSunShadowHeight>
		<blockLight>false</blockLight>
		<stuffCategories>
			<li>Metallic</li>
			<li>Stony</li>
		</stuffCategories>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<isInert>true</isInert>
			<canPlaceOverWall>true</canPlaceOverWall>
			<blueprintGraphicData>
				<texPath>Things/Building/Linked/Embrasure/GW_OrdinaryWalls_Emb_Atlas</texPath>
			</blueprintGraphicData>
			<paintable MayRequire="Rimworld.Biotech">true</paintable>
		</building>
		<comps>
		</comps>
		<disableImpassableShotOverConfigError>true</disableImpassableShotOverConfigError>
	</ThingDef>

