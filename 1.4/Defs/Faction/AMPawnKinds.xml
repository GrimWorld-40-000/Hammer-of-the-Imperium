<?xml version="1.0" encoding="utf-8" ?>
<Defs>
            <!-- Pawn Bases -->
    <PawnKindDef Name="GW_AM_PawnBase" Abstract="True">
        <race>Human</race>
        <defaultFactionType>GW_AM_AstraMilitarum</defaultFactionType>
        <invNutrition>2.55</invNutrition>
        <chemicalAddictionChance>0</chemicalAddictionChance>
        <backstoryCryptosleepCommonality>0.08</backstoryCryptosleepCommonality>
        <itemQuality>Good</itemQuality>
        <gearHealthRange>1~1</gearHealthRange>
        <minGenerationAge>20</minGenerationAge>
        <maxGenerationAge>35</maxGenerationAge>
        <combatEnhancingDrugsChance>0</combatEnhancingDrugsChance>
        <isFighter>true</isFighter>
        <initialWillRange>3~5</initialWillRange>
        <initialResistanceRange>7~11</initialResistanceRange>
        <xenotypeSet Inherit="False">
            <xenotypeChances>
                <Baseliner MayRequire="Ludeon.RimWorld.Biotech">999</Baseliner>
            </xenotypeChances>
        </xenotypeSet>
        <useFactionXenotypes>false</useFactionXenotypes>
        <inventoryOptions>
            <skipChance>0.9</skipChance>
            <subOptionsChooseOne>
                <li>
                    <thingDef>AdvancedMedkit</thingDef>
                    <countRange>1</countRange>
                </li>
                <li>
                    <thingDef>FieldMedkit</thingDef>
                    <countRange>5</countRange>
                </li>
            </subOptionsChooseOne>
        </inventoryOptions>
    </PawnKindDef>

        <!--Tier 1-->
    <PawnKindDef Name="GW_AM_RangedTier1" ParentName="GW_AM_PawnBase" Abstract="True">
        <combatPower>60</combatPower>
        <apparelMoney>1000~1200</apparelMoney>
        <apparelAllowHeadgearChance>0.4</apparelAllowHeadgearChance>
        <weaponTags>
            <li>HP_Autogun</li>
            <li>GW_AM_Lasgun</li>
        </weaponTags>
        <weaponMoney>550~850</weaponMoney>
        <techHediffsChance>0.04</techHediffsChance>
        <techHediffsMoney>750~1050</techHediffsMoney>
        <techHediffsTags>
            <li>Simple</li>
        </techHediffsTags>
        <skills>
            <li>
                <skill>Shooting</skill>
                <range>2~8</range>
            </li>
        </skills>
        <disallowedTraits>
            <li>Brawler</li>
        </disallowedTraits>
        <modExtensions>
            <li Class="CombatExtended.LoadoutPropertiesExtension" MayRequire="ceteam.combatextended">
			    <primaryMagazineCount>
				    <min>3</min>
				    <max>5</max>
			    </primaryMagazineCount>
				<sidearms>
					<li>
						<generateChance>0.5</generateChance>
						<sidearmMoney>
							<min>300</min>
							<max>500</max>
						</sidearmMoney>
						<weaponTags>
							<li>CE_Sidearm_Melee</li>
						</weaponTags>
					</li>
				</sidearms>
		    </li>
        </modExtensions>
    </PawnKindDef>

